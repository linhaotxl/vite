{
  "printWidth": 80,
  "arrowParens": "avoid",
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "parser": "typescript",
  "overrides": [
    {
      "files": "*.json",
      "options": { "parser": "json" }
    },
    {
      "files": ".prettierrc",
      "options": { "parser": "json" }
    }
  ]
}
